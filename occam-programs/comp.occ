PROC read0s(CHAN OF BYTE in, CHAN OF INT out)
  BYTE tmp:
  SEQ
    in ? tmp
    out ! 0 
:

PROC read1s(CHAN OF BYTE in, CHAN OF INT out)
  BYTE tmp:
  SEQ
    in ? tmp
    out ! 1 
:

PROC complex(CHAN OF BYTE in, out, err)
  CHAN OF BYTE in1, in2:
  CHAN OF INT c1, c2:
  BYTE v:
  INT tmp:
  SEQ
    in ? v
    PAR
      SEQ
        in2 ! v
        in1 ! v
      read0s(in1, c1)
      read1s(in2, c2)
      SEQ i = 0 FOR 2
        ALT
          c1 ? tmp
            out ! BYTE(tmp + 48)
          c2 ? tmp
            out ! BYTE(tmp + 48)
:
